{
  "compilerOptions": {
    "allowJs": true,
    "module": "es6",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "jsx": "react",
    "strict": true,
    "sourceMap": true,
    "baseUrl": "./",
    "outDir": "../../../../../../build/dont_build_typisch_renderer_tsconfig",
    "moduleResolution": "node",
    "paths": {
      "*": [
        "*",
        "../common/*",
        "../../../../../../node_modules/*"
      ]
    },
    "target": "ES2020",
    "rootDir": "../../../../../.."
  },
  "include": [
    "**/*",
    "../common/**/*",
  ]
}
